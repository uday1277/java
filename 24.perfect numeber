import java.util.Scanner;
class ex24{
    public static void main(String args[]){
        Scanner Sc=new Scanner(System.in);
        int sum=0;
        System.out.println("enter the number: ");
        int n=Sc.nextInt();
        for(int i=1;i<n;i++){
            if(n%i==0){
                sum=sum+i;

            }
        }if(sum==n){
            System.out.println("perfect numebr");
        }
        else{
            System.out.println("not a perfect numebr");
        }
        }
    }
